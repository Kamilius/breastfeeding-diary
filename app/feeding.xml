    <Label text="{{ elapsedTime }}" cssClass="timer"></Label>
    <StackLayout orientation="horizontal">
      <Switch cssClass="entry--pee"></Switch>
      <Switch cssClass="entry--poo"></Switch>
      <ListPicker cssClass="entry--method"
                  items="{{ ['Груди(л)', 'Груди(п)', 'Пляшечка'] }}"></ListPicker>
    </StackLayout>
  </StackLayout>
</Page>
